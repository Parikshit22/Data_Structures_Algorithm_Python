    " \n",
    "def printPermutations(string,li):\n",
    "    if len(string)==0:\n",
    "        print(li)\n",
    "        return\n",
    "    for i in range(len(string)):\n",
    "        val = string[i]\n",
    "        printPermutations(string[:i]+string[i+1:],li+val)\n",
    "        \n",
    "\n",
    "string = input()\n",
    "printPermutations(string,'')\n",
