   "source": [
    "def search(arr,s,e,x):\n",
    "    if(s>e):\n",
    "        return False\n",
    "    mid = int((s+e)/2)\n",
    "    if arr[mid]==x:\n",
    "        return True\n",
    "    if x<arr[mid]:\n",
    "        return search(arr,s,mid-1,x)\n",
    "    else:\n",
    "        return search(arr,mid+1,e,x)\n"
   ]
