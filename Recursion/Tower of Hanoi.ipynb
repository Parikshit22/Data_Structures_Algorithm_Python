    "def towerofhanoi(n,a,b,c):\n",
    "    if(n==1):\n",
    "        print(f\"move from {a} to {c}\")\n",
    "        return\n",
    "    towerofhanoi(n-1,a,c,b)\n",
    "    print(f\"move from {a} to {c}\")\n",
    "    towerofhanoi(n-1,b,a,c)"
   ]
  },
